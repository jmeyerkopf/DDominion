%YAML 1.1
%TAG !u! tag:unity3d.com,2011:
--- !u!104 &1
EditorBuildSettings:
  m_ObjectHideFlags: 0
  serializedVersion: 2
  m_Scenes:
  - enabled: 1
    path: Assets/Scenes/MainScene.unity # Placeholder for our scene
  m_ActiveBuildTarget: Android # Options: Standalone, WebGL, iOS, Android, etc.
  m_BuildTargetPlatform: Android
  # Specific platform settings might be nested here or in separate files
  # For example, for Android:
  # m_AndroidBuildSystem: Gradle
  # m_AndroidMinSdkVersion: 22
  # m_AndroidTargetSdkVersion: 33
  # m_AndroidSplashScreenScale: 0 # Center
  # m_AndroidKeystoreName:
  # m_AndroidKeyaliasName:
  # ... and other build settings like compression, symbols, etc.
--- !u!162 &2 # PlatformBuildSettings
PlatformBuildSettings:
  m_ObjectHideFlags: 0
  buildTarget: Android
  buildTargetGroup: Android
  # Add specific settings for Android platform if needed
  # For example:
  # textureCompression: ETC2_RGB4
  # buildSystem: Gradle
  # androidBundleVersionCode: 1
  # androidMinSdkVersion: 22
  # androidTargetSdkVersion: 33
  # etc.
--- !u!162 &3 # PlatformBuildSettings
PlatformBuildSettings:
  m_ObjectHideFlags: 0
  buildTarget: iOS
  buildTargetGroup: iOS
  # Add specific settings for iOS platform if needed
  # For example:
  # targetDevice: iPhoneAndiPad
  # targetOSVersionString: 12.0
  # cameraUsageDescription: Used for AR features
  # etc.
--- !u!162 &4 # PlatformBuildSettings
PlatformBuildSettings:
  m_ObjectHideFlags: 0
  buildTarget: StandaloneWindows64
  buildTargetGroup: Standalone
  # Add specific settings for Standalone platform if needed
  # For example:
  # displayResolutionDialog: Enabled
  # fullscreenMode: FullscreenWindow
  # etc.
# Add other platforms as needed (WebGL, Linux, macOS)
# --- !u!162 &5
# PlatformBuildSettings:
# m_ObjectHideFlags: 0
# buildTarget: WebGL
# buildTargetGroup: WebGL
# ... settings for WebGL
#
# --- !u!162 &6
# PlatformBuildSettings:
# m_ObjectHideFlags: 0
# buildTarget: StandaloneLinux64
# buildTargetGroup: Standalone
# ... settings for Linux64
#
# --- !u!162 &7
# PlatformBuildSettings:
# m_ObjectHideFlags: 0
# buildTarget: StandaloneOSX
# buildTargetGroup: Standalone
# ... settings for macOSX
#
# It's important that m_ActiveBuildTarget and m_BuildTargetPlatform in EditorBuildSettings
# match one of the buildTarget fields in a PlatformBuildSettings entry.
# The actual settings within each PlatformBuildSettings can be quite extensive
# and are normally managed by the Unity Editor.
# For a minimal setup, only the target platform sections might be needed,
# and often, they are sparsely populated initially.
